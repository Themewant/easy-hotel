{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "easy-hotel/accomodationinfo",
  "version": "0.1.0",
  "title": "Easy Hotel Accomodation Info",
  "category": "easy-hotel",
  "icon": "calendar-alt",
  "description": "Basic info for easy hotel room.",
  "example": {},
  "supports": {
    "html": false,
    "align": true,
    "layout": true
  },
  "selectors": {
    "root": ".eshb-accomodation-info-wrapper",
    "color": {
      "text": ".eshb-accomodation-info-wrapper .field-label",
      "background": ".eshb-accomodation-info-wrapper",
      "link": ".eshb-accomodation-info-wrapper a"
    },
    "typography": {
      "root": ".eshb-accomodation-info-wrapper .field-label",
      "text": ".eshb-accomodation-info-wrapper .field-label"
    },
    "spacing": {
      "root": ".eshb-accomodation-info-wrapper",
      "padding": ".eshb-accomodation-info-wrapper",
      "margin": ".eshb-accomodation-info-wrapper"
    }
  },
  "textdomain": "easy-hotel",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "attributes": {
    "spaceBetween": {
      "type": "string",
      "default": "20"
    },
    "textColor": {
      "type": "string",
      "default": ""
    },
    "textColorHover": {
      "type": "string",
      "default": ""
    },
    "textSize": {
      "type": "string",
      "default": ""
    },
    "iconSize": {
      "type": "string",
      "default": "25"
    },
    "iconColor": {
      "type": "string",
      "default": ""
    },
    "iconColorHover": {
      "type": "string",
      "default": ""
    },
    "iconSpace": {
      "type": "string",
      "default": "5"
    }
  }
}