{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "easy-hotel/searchform",
    "version": "0.1.0",
    "title": "Easy Hotel Search",
    "category": "easy-hotel",
    "icon": "search",
    "description": "A search form for easy hotel rooms.",
    "example": {},
    "supports": {
        "html": false,
        "align": true,
        "layout": true,
        "spacing": {
            "margin": true,
            "padding": true
        },
        "color": {
            "background": true,
            "text": true,
            "link": true
        },
        "typography": {
            "fontSize": true,
            "fontWeight": true,
            "fontStyle": true,
            "lineHeight": true,
            "textDecoration": true,
            "textTransform": true
        }
    },
    "selectors": {
        "root": ".eshb-search",
        "color": {
            "text": ".eshb-search .field-label",
            "background": ".eshb-search",
            "link": ".eshb-search a"
        },
        "typography": {
            "root": ".eshb-search .field-label",
            "text": ".eshb-search .field-label"
        },
        "spacing": {
            "root": ".eshb-search",
            "padding": ".eshb-search",
            "margin": ".eshb-search"
        }
    },
    "textdomain": "easy-hotel",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": [
        "file:./style.scss",
        "eshb-style"
    ],
    "render": "file:./render.php",
    "attributes": {
        "customBackgroundColor": {
            "type": "string",
            "default": ""
        }
    }
}