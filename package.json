{
	"name": "easy-hotel-blocks",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "npm run build:accomodationgrid && npm run build:searchform && npm run build:bookingform",
		"build:accomodationgrid": "wp-scripts build --blocks-manifest --webpack-src-dir=public/includes/gutenberg/blocks/accomodationgrid/src --output-path=public/includes/gutenberg/blocks/accomodationgrid/build",
		"build:searchform": "wp-scripts build --blocks-manifest --webpack-src-dir=public/includes/gutenberg/blocks/searchform/src --output-path=public/includes/gutenberg/blocks/searchform/build",
		"build:bookingform": "wp-scripts build --blocks-manifest --webpack-src-dir=public/includes/gutenberg/blocks/bookingform/src --output-path=public/includes/gutenberg/blocks/bookingform/build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --blocks-manifest --webpack-src-dir=public/includes/gutenberg/blocks/accomodationgrid/src --output-path=public/includes/gutenberg/blocks/accomodationgrid/build"
	},
	"devDependencies": {
		"@wordpress/scripts": "^31.1.0"
	}
}